<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!--
    Replacing the "M0 0" with "M0z" in the below path makes Samsung
    Experience-Start crash repeatedly, which in turn makes the phone
    unusable because the Home screen cannot be rendered anymore.

    Omitting the whole path element makes several Android phones fall back
    to the Android default app icon, showing a robot head.
    -->
    <path
        android:fillColor="@android:color/transparent"
        android:pathData="M0 0" />
</vector>
